<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- VISTA TREE DE LOS MODELOS  -->
        <record model="ir.ui.view" id="ga_apoderado_list" >
            <field name="name">Ga Apoderado List</field>
            <field name="model">ga.apoderado</field>
            <field name="arch" type="xml">
                <tree string="Apoderado" create="true" edit="true">
                    <field name="codigo"/>
                    <field name="ci"/>
                    <field name="name"/>
                    <field name="telefono"/>
                    <field name="parentesco"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="ga_alumno_list" >
            <field name="name">Ga Alumno List</field>
            <field name="model">ga.alumno</field>
            <field name="arch" type="xml">
                <tree string="Alumno" create="true" edit="true">
                    <field name="codigo"/>
                    <field name="ci"/>
                    <field name="name"/>
                    <field name="telefono"/>
                    <field name="correo"/>
                </tree>
            </field>
        </record>


        <record model="ir.ui.view" id="ga_chofer_list" >
            <field name="name">Ga Chofer List</field>
            <field name="model">hr.employee</field>
            <field name="arch" type="xml">
                <tree string="Chofer" create="true" edit="true">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="ga_chofer_list" >
            <field name="name">Ga Chofer List</field>
            <field name="model">hr.employee</field>
            <field name="arch" type="xml">
                <tree string="Chofer " create="true" edit="true">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="ga_registro_academico_list" >
            <field name="name">Ga Registro Academico List</field>
            <field name="model">ga.registro.academico</field>
            <field name="arch" type="xml">
                <tree string="Registro Academico " create="true" edit="true">
                    <field name="fecha_entrega"/>
                    <field name="calificacion"/>
                    <field name="alumno_id"/>
                    <field name="actividad_academica_id"/>
                </tree>
            </field>
        </record>
        

        <record model="ir.ui.view" id="ga_inscripcion_list" >
            <field name="name">Ga Inscripcion List</field>
            <field name="model">ga.inscripcion.alumno</field>
            <field name="arch" type="xml">
                <tree string="Inscripcion " create="true" edit="true">
                    <field name="gestion_id"/>
                    <field name="grado_id"/>
                    <field name="paralelo_id"/>
                    <field name="apoderado_id"/>
                    <field name="alumno_id"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="ga_ruta_chofer_list" >
            <field name="name">Ga Ruta Chofer List</field>
            <field name="model">ga.ruta.chofer</field>
            <field name="arch" type="xml">
                <tree string="Ruta Chofer " create="true" edit="true">
                    <field name="chofer_id"/>
                    <field name="alumno_id"/>
                    <field name="ruta_id"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="ga_ruta_list" >
            <field name="name">Ga Ruta List</field>
            <field name="model">ga.ruta</field>
            <field name="arch" type="xml">
                <tree string="Chofer " create="true" edit="true">
                    <field name="codigo"/>
                    <field name="descripcion"/>
                    <field name="hora_inicio"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="ga_actividad_academica_list" >
            <field name="name">Ga Acatividad Academica List</field>
            <field name="model">ga.actividad.academica</field>
            <field name="arch" type="xml">
                <tree string="Actividad Academica " create="true" edit="true">
                    <field name="codigo"/>
                    <field name="descripcion"/>
                    <field name="fecha_registro"/>
                    <field name="fecha_entrega"/>
                    <field name="ponderacion"/>
                    <field name="paralelo_profesor_id"/>
                    <field name="ciclo_academico_id"/>
                </tree>
            </field>
        </record>



        <!-- FORMULARIOS DE EDICION DE O DE CREACION  -->
        <record model="ir.ui.view" id="ga_apoderado" >
            <field name="name">Ga Apoderado Form</field>
            <field name="model">ga.apoderado</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                    <field name="codigo"/>
                    <field name="ci"/>
                    <field name="name"/>
                    <field name="fecha_nacimiento"/>
                    <field name="ocupacion"/>
                    <field name="telefono"/>
                    <field name="parentesco"/>
                    </group>
                </form>
            </field>
        </record>




        <record model="ir.ui.view" id="ga_alumno" >
            <field name="name">Ga Alumno Form</field>
            <field name="model">ga.alumno</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                    <field name="codigo"/>
                    <field name="ci"/>
                    <field name="name"/>
                    <field name="fecha_nacimiento"/>
                    <field name="genero"/>
                    <field name="nacionalidad"/>
                    <field name="domicilio"/>
                    <field name="telefono"/>
                    <field name="correo"/>
                    </group>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="ga_inscripcion" >
            <field name="name">Ga Inscripcion Form</field>
            <field name="model">ga.inscripcion.alumno</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                    <field name="gestion_id"/>
                    <field name="grado_id"/>
                    <field name="paralelo_id"/>
                    <field name="apoderado_id" />
                    <field name="alumno_id"/>
                    </group>
                </form>
            </field>
        </record>


        <record model="ir.ui.view" id="ga_ruta_chofer" >
            <field name="name">Ga Ruta Chofer Form</field>
            <field name="model">ga.ruta.chofer</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                    <field name="chofer_id"/>
                    <field name="alumno_id"/>
                    <field name="ruta_id"/>
                    </group>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="ga_ruta" >
            <field name="name">Ga Ruta Form</field>
            <field name="model">ga.ruta</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                    <field name="codigo"/>
                    <field name="descripcion"/>
                    <field name="hora_inicio"/>
                    </group>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="ga_registro_academico" >
            <field name="name">Ga Registro Academico Form</field>
            <field name="model">ga.registro.academico</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                    <field name="fecha_entrega"/>
                    <field name="actividad_academica_id"/>
                    </group>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="ga_actividad_academica" >
            <field name="name">Ga Actividad Academica Form</field>
            <field name="model">ga.actividad.academica</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="codigo"/>
                        <field name="descripcion"/>
                        <field name="fecha_registro"/>
                        <field name="fecha_entrega"/>
                        <field name="ponderacion"/>
                        <field name="paralelo_profesor_id"/>
                        <field name="ciclo_academico_id"/>
                    </group>
                </form>
            </field>
        </record>

        <!-- BOTONES DE ACCION -->
        
        <record model="ir.actions.act_window" id="ga.action_apoderado_window" >
            <field name="name">Apoderados</field>
            <field name="res_model">ga.apoderado</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record model="ir.actions.act_window" id="ga.action_alumno_window" >
            <field name="name">Alumnos</field>
            <field name="res_model">ga.alumno</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record model="ir.actions.act_window" id="ga.action_inscripcion_window" >
            <field name="name">Alumnos Inscritos</field>
            <field name="res_model">ga.inscripcion.alumno</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record model="ir.actions.act_window" id="ga.action_chofer_window" >
            <field name="name">Choferes</field>
            <field name="res_model">hr.employee</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record model="ir.actions.act_window" id="ga.action_ruta_chofer_window" >
            <field name="name">Rutas Choferes</field>
            <field name="res_model">ga.ruta.chofer</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record model="ir.actions.act_window" id="ga.action_ruta_window" >
            <field name="name">Rutas</field>
            <field name="res_model">ga.ruta</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record model="ir.actions.act_window" id="ga.action_registro_academico_window" >
            <field name="name">Registro Academico</field>
            <field name="res_model">ga.registro.academico</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record model="ir.actions.act_window" id="ga.action_actividad_academica_window" >
            <field name="name">Actividad Academica</field>
            <field name="res_model">ga.actividad.academica</field>
            <field name="view_mode">tree,form</field>
        </record>

        <!-- BOTONES DE NAVEGACION -->

        <menuitem name= "Gestion Academica" id="ga.menu_root"/>
        
        <menuitem name= "Creacion" id="ga.menu_1" parent="ga.menu_root"/>
        <menuitem name= "Apoderado" id="ga.menu_1_apoderado_list" parent="ga.menu_1" action="ga.action_apoderado_window"/>
        <menuitem name= "Alumno" id="ga.menu_1_alumno_list" parent="ga.menu_1" action="ga.action_alumno_window"/>
        <menuitem name= "Inscritos" id="ga.menu_1_inscripcion_list" parent="ga.menu_1" action="ga.action_inscripcion_window"/>
        <menuitem name= "Chofer" id="ga.menu_1_chofer_list" parent="ga.menu_1" action="ga.action_chofer_window"/>
        <menuitem name= "Rutas Chofer" id="ga.menu_1_ruta_chofer_list" parent="ga.menu_1" action="ga.action_ruta_chofer_window"/>
        <menuitem name= "Rutas" id="ga.menu_1_ruta_list" parent="ga.menu_1" action="ga.action_ruta_window"/>
        <menuitem name= "Registro Academico" id="ga.menu_1_registro_academico_list" parent="ga.menu_1" action="ga.action_registro_academico_window"/>
        <menuitem name= "Actividad Academica" id="ga.menu_1_actividad_academica_list" parent="ga.menu_1" action="ga.action_actividad_academica_window"/>
        

        
    </data>
</odoo>